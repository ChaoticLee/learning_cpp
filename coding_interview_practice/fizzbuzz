#include <iostream>
#include <stdlib.h>
#include <cmath>

// the point of fizzbuzz is to output numbers 1 to 100 and then for any multiple of 3, print fizz and for any multiple of 5, print buzz, and any numbers that are multiples of 3 and 5, print Fizzbuzz

int main()
{
int fizz,buzz; 

for(int i = 1; i <=100; ++i){

  if(i % 3 == 0 && i%5 == 0){
    std::cout <<"FizzBuzz\n";
  }
  else if(i % 3 == 0)
  {
    std::cout <<"Fizz\n";
  }
  else if(i%5 ==0)
  {
    std::cout <<"Buzz\n";
  }
  else {
    std::cout <<i<<"\n";
  }
}
return 0; 
}
